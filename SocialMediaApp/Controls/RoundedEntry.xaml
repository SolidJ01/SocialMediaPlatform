<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Name="this"
             x:Class="SocialMediaApp.Controls.RoundedEntry">
    <Frame BindingContext="{x:Reference this}" CornerRadius="10" IsClippedToBounds="True" Padding="0" Margin="0,20,0,0">
        <Entry Text="{Binding Text, Mode=TwoWay}" Style="{StaticResource TextEntry}" />
    </Frame>
</ContentView>
